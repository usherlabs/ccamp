type ParsedHeader = record {
    name: text;
    value: text;
};

type ParsedRequest = record {
    method: opt text;
    path: opt text;
    version: opt nat8;
    headers: vec ParsedHeader;
};

type ParsedResponse = record {
    version: opt nat8;
    code: opt nat16;
    reason: opt text;
    headers: vec ParsedHeader;
    body: text;
};

service : {
  verify_data_proof : (text) -> (text, text);
  verify_tls_proof : (text) -> (ParsedRequest, ParsedResponse, text);
}